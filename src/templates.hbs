<li class="main">
    <h2>{{name}}</h2>
    <div>
        <p>Capital: {{capital}}</p>
        <p>Population: {{population}}</p>
        <ul><span>Languages: </span>
        {{#each languages}}
            <li>{{this.name}}</li>
        {{/each}}
        </ul>
    </div>
    <div>
        <img src="{{flag}}" alt="flag" width="300px" >
    </div>
</li>